{
  "version": 2,
  "name": "akese-akoben",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],

  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "PRODUCTION_FRONTEND_URL": "@production_frontend_url"
  }
}
