{
  "version": 2,
  "name": "akese-akoben",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "src/routes/**/*.js": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "PRODUCTION_FRONTEND_URL": "@production_frontend_url"
  }
}
